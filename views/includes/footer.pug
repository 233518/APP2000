footer(uk-sticky='bottom: true')
    .footer-wrapper
        .footerlogo
            img(src='/images/logo/footerlogo.png')
        ul.uk-list.footer-list
            a(href=`/${urlPath}/infosider/about`)
                li #{__("FOOTER_ABOUT_US")}
            a(href=`/${urlPath}/infosider/about#contact-form`)
                li #{__("FOOTER_CONTACT_US")}
            a(href=`#`)
                li #{__("FOOTER_PRIVACY_POLICY")}
            a(href=`#`)
                li #{__("FOOTER_TERMS_OF_USE")}
        .socials
            h4.uk-align-center #{__("FOOTER_SOCIAL_TITLE")}
            .footer-socials-wrapper
                a(href='http://www.facebook.com')
                    img(src='/images/logo/facebooklogo.png' height='35px' width='35px')
                a(href='http://www.twitter.com')
                    img(src='/images/logo/twitterlogo.png' height='35px' width='35px')
                a(href='http://www.instagram.com')
                    img(src='/images/logo/instagramlogo.png' height='35px' width='35px')

