- var allFavorites = allFavorites;
p #{__("DASHBOARD_FAVORITES_DESC")}
.uk-grid.uk-width-1-1
    .uk-grid.uk-child-width-1-3
        each item in allFavorites
            .uk-card.uk-card-default.dashboard-favorite-padding
                .uk-card-media-top
                    img(src=`https://www.themoviedb.org/t/p/w600_and_h900_bestv2/${item.pictureUrl}` alt='')
                .uk-card-body
                    h3.uk-card-title= item.title
                    p= item.releaseDate
